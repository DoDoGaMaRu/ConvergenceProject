<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.StoreRegistMapper">
    <resultMap id="result" type="StoreRegistDTO">
        <result property="id" column="id"/>
        <result property="status" column="status"/>
        <result property="comment" column="comment"/>
        <result property="name" column="name"/>
        <result property="tag" column="tag"/>
        <result property="phone" column="phone"/>
        <result property="address" column="address"/>
        <result property="user_id" column="user_id"/>
    </resultMap>

    <insert id="insert">
        insert into order (id, status, tag, carry_price, phone, address, user_id, carry_sale)
        values (#{id}, #{status}, #{tag}, #{carry_price}), #{phone}, #{address), #{user_id}, #{carry_sale})
    </insert>

    <select id="selectAll">
        select * from store_regist
    </select>

    <select id="selectAllForAccept">
        select * from store_regist where status = #{status}
    </select>

    <update id="update">

    </update>
</mapper>